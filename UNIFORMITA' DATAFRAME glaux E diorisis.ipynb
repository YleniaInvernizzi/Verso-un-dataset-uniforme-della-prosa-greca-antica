    "import os\n",
    "import glob\n",
    "import pandas as pd\n",
    "import xml.etree.ElementTree as ET\n",
    "import xmltodict\n",
    "import unicodedata\n",
    "import random #mi serve per prendere di 1420 elementi il 30%\n",
    "\n",
    "\n",
    "def parse_xml_file(file_path):\n",
    "\n",
    "    with open(file_path,'r',encoding='utf-8') as f:\n",
    "        data = f.read()\n",
    "        xml_dict = xmltodict.parse(data)\n",
    "\n",
    "    # 2. Estrazione e normalizzazione\n",
    "    lista_glaux = []\n",
    "\n",
    "    for i, sent in enumerate(xml_dict['treebank']['sentence']):\n",
    "        words = sent['word']\n",
    "\n",
    "        if isinstance(words, list):\n",
    "            df = pd.DataFrame(words)\n",
    "        else:\n",
    "            df = pd.DataFrame([words])\n",
    "\n",
    "\n",
    "        for col in ['@form', '@lemma', '@postag', '@relation']:\n",
    "            if col in df.columns:\n",
    "                df[col] = df[col].apply(lambda x: unicodedata.normalize(\"NFC\", x) if isinstance(x, str) else x)\n",
    "\n",
    "        lista_glaux.append(df)\n",
    "\n",
    "    # 3. Concatenazione\n",
    "    glaux_df = pd.concat(lista_glaux, ignore_index=True)\n",
    "    return glaux_df[['@id', '@form', '@lemma', '@postag']]\n",
    "\n",
