   "source": [
    "import ast\n",
    "import string\n",
    "\n",
    "# Get the intersection of forms\n",
    "intersected_forms = set(glaux_df['@form']).intersection(set(gorman_df['form']))\n",
    "\n",
    "# Create a dictionary with 'lemmi_gorman' for the intersected forms\n",
    "result_dict = {}\n",
    "for form in intersected_forms:\n",
    "    # Find the corresponding row in gorman_df\n",
    "    gorman_row = gorman_df[gorman_df['form'] == form].iloc[0]\n",
    "    \n",
    "    # Convert the string representation of the list to an actual list\n",
    "    lemmi_gorman_list = ast.literal_eval(gorman_row['lemmi_gorman'])\n",
    "    \n",
    "    result_dict[form] = {\n",
    "        'lemmi_gorman': lemmi_gorman_list,\n",
    "        'lemmi_glaux': []  # Initialize with empty list for lemmi_glaux\n",
    "    }\n",
    "\n",
    "# Now, populate 'lemmi_glaux' from glaux_df for the intersected forms\n",
    "for form in intersected_forms:\n",
    "    # Find the corresponding rows in glaux_df\n",
    "    glaux_rows = glaux_df[glaux_df['@form'] == form]\n",
    "    # Extract unique lemmas from glaux_df rows and add to the dictionary\n",
    "    lemmi_glaux = sorted(list(set(glaux_rows['@lemma'].dropna())))\n",
    "    result_dict[form]['lemmi_glaux'] = lemmi_glaux\n",
    "\n",
    "# Remove punctuation from dictionary keys\n",
    "result_dict_no_punct = {}\n",
    "translator = str.maketrans('', '', string.punctuation)\n",
    "for form, data in result_dict.items():\n",
    "    cleaned_form = form.translate(translator)\n",
    "    result_dict_no_punct[cleaned_form] = data\n",
    "\n",
    "result_dict = result_dict_no_punct\n"
   ]
  },
