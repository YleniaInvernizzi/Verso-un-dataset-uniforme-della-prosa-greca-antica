    "#         all_df.append(single_data)\n",
    "#     return all_df\n",
    "\n",
    "#         #single_data.to_csv(full_path, sep=';') #SCOMMENTA PER SALVARE\n",
    "\n",
    "\n",
    "\n",
    "def parse_all_xmls_in_directory(directory_path, subset):\n",
    "    all_df = []\n",
    "    xml_list = glob.glob(os.path.join(directory_path, \"*.xml\"))\n",
    "    n = int(len(xml_list)*subset)\n",
    "    xml_list_sub_sample = random.sample(xml_list, n)\n",
    "    for xml_file in xml_list_sub_sample:\n",
    "        try:\n",
    "            single_data = parse_xml_file(xml_file)\n",
    "            print(f\"✅ File processato: {xml_file}\")\n",
    "            all_df.append(single_data)\n",
    "        except Exception as e:\n",
    "            print(f\"⚠️ Errore nel file: {xml_file}\")\n",
    "            print(f\"   Dettaglio errore: {e}\")\n",
    "            # continua con il prossimo file\n",
    "            continue\n",
    "    return all_df\n",
    "\n"
